{
  "name": "draft-ai",
  "version": "0.0.1",
  "description": "Node backend project",
  "author": "Denis Iakimenko",
  "private": true,
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "start:dev": "nodemon src/index.ts",
    "start:deploy": "read alias && firebase use $alias && firebase deploy --only functions:ai,hosting:$alias-ai",
    "start:debug": "nodemon --inspect=0.0.0.0:9229 src/index.ts",
    "start:functions": "npm run build:prod && npx @google-cloud/functions-framework --target=ai",
    "build:prod": "tsc -p tsconfig.json && mkdir -p dist/nsfw && cp -R src/nsfw dist"
  },
  "keywords": [
    "node",
    "fastify",
    "nsfw",
    "ai",
    "swagger",
    "javascript"
  ],
  "dependencies": {
    "@fastify/compress": "^6.2.1",
    "@fastify/cors": "^8.2.1",
    "@fastify/env": "^4.2.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^2.1.0",
    "@tensorflow/tfjs-node": "^4.17.0",
    "busboy": "^1.6.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.7.0",
    "nsfwjs": "^2.4.2",
    "openai": "^4.26.0"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.3",
    "@types/node": "^18.16.1",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fastify-tsconfig": "^2.0.0",
    "firebase-functions-test": "^3.1.1",
    "nodemon": "^3.0.1",
    "npm-check-updates": "^16.14.5",
    "pino-pretty": "^10.0.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
